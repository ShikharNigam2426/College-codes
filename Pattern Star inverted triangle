#include <stdio.h>

int main() {
    // Write C code here
    int n;
    scanf("%d",&n);
    int space = 0;
    for(int i=0;i<n;i++)
    {
        for(int j=1;j<=i;j++)
        {
            printf("  ");
        }
        for(int k = i;k<n;k++)
        {
            printf("* ");
        }
        printf("\n");
    }

    return 0;
}
